procedure pStartServer;
sCode : SysCode;
begin
    sCode := StartFTPServer;
    if (sCode = SysOk) then
        DisplayStatus("Server activo");
        SetTimerMode(1, TimerContinuous);
        SetTimer(1, 300);
        StartTimer(1);

        SetTimerMode(2, TimerContinuous);
        SetTimer(2, 300);
        StartTimer(2);

    else
        DisplayStatus(SysCodeToString(sCode));
    end if;    
end;
